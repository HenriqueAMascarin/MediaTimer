{
  "expo": {
    "name": "Media Timer",
    "slug": "media-timer",
    "description": "This is an timer music app",
    "icon": "./assets/icon.png",
    "version": "1.0.9",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptativeIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.henriqueamascarin.mediatimer",
      "edgeToEdgeEnabled": true
    },
    "web": {},
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "../../node_modules/@notifee/react-native/android/libs"
            ]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#149CFF",
          "image": "./assets/icon.png",
          "imageWidth": 240
        }
      ],
      "expo-localization",
      "expo-audio",
      "expo-font",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
